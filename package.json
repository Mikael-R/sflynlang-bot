{
  "name": "sflynlang-bot",
  "version": "1.0.0",
  "description": "Official Discord bot for Sflynlang server.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "nodemon",
    "nodemon": "npm run build && npm run start:ts",
    "start": "node ./dist/index.js",
    "start:ts": "ts-node ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sflynlang/sflynlang-bot.git"
  },
  "keywords": [
    "DiscordJS",
    "Sflynlang"
  ],
  "author": "Sflynlang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sflynlang/sflynlang-bot/issues"
  },
  "homepage": "https://github.com/sflynlang/sflynlang-bot#readme",
  "engines": {
    "node": ">=12.x"
  },
  "_moduleAliases": {
    "@Bot": "dist/Bot.js",
    "@Commands": "dist/commands",
    "@Database": "dist/database",
    "@Decorators": "dist/decorators",
    "@Events": "dist/events",
    "@Interfaces": "dist/interfaces"
  },
  "dependencies": {
    "discord.js": "^12.3.1",
    "module-alias": "^2.2.2",
    "moment": "^2.28.0",
    "mongoose": "^5.10.6",
    "oneline": "^1.0.3"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.1",
    "@types/ws": "^7.2.6",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
